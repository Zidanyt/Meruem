generator client {
  provider = "prisma-client-js"
}

datasource db {
  provider = "mongodb"
  url      = env("DATABASE_URL")
}

model Point {
  id           String   @id @map("_id")
  city         String
  cnpj         String?
  createdAt    DateTime @db.Date
  latitude     Float
  longitude    Float
  name         String
  neighborhood String
  street       String
  updatedAt    DateTime @db.Date
  userId       String
}

model User {
  id       String @id @default(auto()) @map("_id") @db.ObjectId
  email    String @unique
  name     String
  password String
}
